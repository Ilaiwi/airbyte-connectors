{
  "documentationUrl": "https://docs.faros.ai",
  "connectionSpecification": {
    "$schema": "http://json-schema.org/draft-07/schema#",
    "title": "TravisCI Spec",
    "type": "object",
    "required": [
      "token",
      "organization",
      "cutoff_days"
    ],
    "additionalProperties": false,
    "properties": {
      "token": {
        "type": "string",
        "title": "token",
        "description": "TravisCI token",
        "airbyte_secret": true
      },
      "organization": {
        "type": "string",
        "title": "organization",
        "description": "TravisCI organization"
      },
      "cutoff_days": {
        "type": "integer",
        "title": "Cutoff Days",
        "default": 90,
        "description": "Only fetch data updated after cutoff"
      },
      "api_url": {
        "type": "string",
        "title": "API URL",
        "description": "TravisCI API URL"
      },
      "api_version": {
        "type": "string",
        "title": "API Version",
        "description": "TravisCI API Version"
      },
      "page_size": {
        "type": "integer",
        "title": "Page Size",
        "description": "Page size to use when requesting records from TravisCI API"
      }
    }
  }
}
